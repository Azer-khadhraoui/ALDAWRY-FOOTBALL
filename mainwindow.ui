     <string notr="true">/* Style for the QTabWidget pane (content area) */
QTabWidget#tabWidget::pane {
    background-color: #1E2633; /* Match the form widget background */
    border: 2px solid #2A3A4A; /* Subtle blue-gray border */
    border-radius: 5px;
}

/* Style for the tab bar (tabs) */
QTabBar::tab {
    background-color: #252A38; /* Slightly lighter than the pane */
    color: #B0C0D0; /* Light grayish-blue for inactive tab text */
    padding: 10px 20px; /* Increased padding for better spacing */
    border: 1px solid #2A3A4A; /* Subtle border */
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    margin-right: 2px; /* Space between tabs */
    font-size: 12px;
    font-weight: 500;
}

/* Style for the selected tab */
QTabBar::tab:selected {
    background-color: #2E3440; /* Darker shade for the active tab */
    color: #E0E6ED; /* Brighter text for the active tab */
    font-weight: bold;
    border-bottom: 2px solid #00FF7F; /* Green underline to match the accent */
}

/* Style for tab on hover */
QTabBar::tab:hover {
    background-color: #353F4D; /* Lighter blue-gray on hover */
    color: #E0E6ED;
}

/* Style for non-selected tabs */
QTabBar::tab:!selected {
    margin-top: 2px; /* Slightly lower non-selected tabs */
    color: #A0A8B7; /* Dimmer text for non-selected tabs */
}

/* Style for the tab bar's corner button (if any) */
QTabWidget#tabWidget QTableCornerButton::section {
    background-color: #252A38;
    border: 1px solid #2A3A4A;
}
QTabBar::tab:hover {
    background: #3d5a73; /* Slight highlight on hover */
}

QTabBar::tab:selected {
    background: #34495e;
    font-weight: bold;
}
/* Selected tab */
QTabBar::tab:selected {
    background: #4a6a89;
    color: #ffffff;
}

/* Unselected tabs */
QTabBar::tab:!selected {
    margin-top: 2px;
}</string>
