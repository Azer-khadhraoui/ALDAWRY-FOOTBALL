    <property name="styleSheet">
     <string notr="true">/* Style for the specific QTableView named tableView */
#tableView {
    background-color: #1C1F2A; /* Matches the dark background of your UI */
    border: 1px solid #2A2D3A; /* Subtle border for definition */
    border-radius: 5px; /* Rounded corners to match your UI */
    color: white; /* White text for readability on dark background */
    gridline-color: #2A2D3A; /* Dark grid lines for better visibility */
    selection-background-color: #4A5970; /* Slightly lighter blue-gray for selected rows */
    selection-color: white; /* White text for selected items */
    alternate-background-color: #192231; /* Alternate row color for better readability */
}

/* Style for headers (horizontal and vertical) of tableView */
#tableView QHeaderView::section {
    background-color: #2E3B4E; /* Slightly lighter header background */
    border: 1px solid #4C5C75; /* Matching border color from your UI */
    border-radius: 3px; /* Subtle rounding */
    color: white; /* White text for headers */
    padding: 5px; /* Padding for better spacing */
    font-weight: bold; /* Bold headers for emphasis */
    margin: 0px; /* Remove extra margins */
}

/* Style for items (cells) in tableView */
#tableView::item {
    padding: 5px; /* Padding inside cells */
}

/* Style for scroll bars of tableView */
#tableView::verticalScrollBar, #tableView::horizontalScrollBar {
    background-color: #1C1F2A; /* Dark background for scroll bars */
    width: 15px; /* Width of vertical scroll bar */
    height: 15px; /* Height of horizontal scroll bar */
}

#tableView QScrollBar::handle:vertical, #tableView QScrollBar::handle:horizontal {
    background-color: #4C5C75; /* Scroll handle color matching your UI lines */
    border-radius: 7px; /* Rounded scroll handle */
    min-height: 20px;
    min-width: 20px;
}

#tableView QScrollBar::add-line, #tableView QScrollBar::sub-line {
    background: none; /* Remove arrows */
    border: none;
}

#tableView QScrollBar::add-page, #tableView QScrollBar::sub-page {
    background: none; /* Transparent background for scroll areas */
}

/* Focus frame (when an item in tableView is focused) */
#tableView:focus {
    border: 2px solid #00FF7F; /* Green outline for focus, matching your logo */
}

/* Disabled state for tableView */
#tableView:disabled {
    color: #808080; /* Gray text for disabled state */
    background-color: #151A24; /* Slightly darker background for disabled state */
}</string>
